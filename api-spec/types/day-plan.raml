#%RAML 1.0 DataType
type: object
displayName: DayPlan
properties:
  day_number:
    type: integer
    required: true
  description:
    type: string
    required: false
    example: "Si diena skirta atsistatymui"
  meals:
    type: array
    required: true
    items:
      type: Meal
      required: true
  exercises:
    type: array
    required: true
    items:
      type: Exercise
      required: true
